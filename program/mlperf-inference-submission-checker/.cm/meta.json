{
  "no_compile": "yes",
  "no_target_file": "yes",
  "process_in_tmp": "yes",
  "program": "yes",
  "run_cmds": {
    "default": {
      "ignore_return_code": "yes",
      "run_time": {
        "skip_print_execution_time":"yes",
        "run_cmd_main": "../_run$#script_ext#$",
        "#run_cmd_out1": "tmp-output1.tmp",
        "run_output_files": []
      }
    }
  },
  "run_deps": {
