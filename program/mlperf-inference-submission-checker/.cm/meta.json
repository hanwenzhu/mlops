      "tags": "mlperf,inference,source"
    },
    "mlperf-inference-results": {
      "force_target_as_host": "yes",
      "local": "yes",
      "name": "MLPerf Inference results",
      "sort": 20,
      "tags": "mlperf,inference,results"
    }
  },
  "run_vars": {},
  "tags": [
    "lang-python",
    "mlperf",
    "mlperf-inference-submission"
  ]
}
