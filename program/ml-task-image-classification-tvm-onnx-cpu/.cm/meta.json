{
  "no_compile": "yes",
  "no_target_file": "yes",
  "process_in_tmp": "yes",
  "program": "yes",
  "print_files_after_run": [
    "stdout.log",
    "stderr.log"
  ],
  "run_cmds": {
    "install-python-requirements": {
      "ignore_return_code": "no",
      "run_time": {
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ -m pip install -r ../requirements.txt",
        "min_run": "yes"
      },
      "run_deps": {
         "python": {
           "add_to_path": "yes",
           "force_target_as_host": "yes",
           "local": "yes",
           "name": "Python interpreter",
           "sort": 0,
           "tags": "compiler,python"
         }
      },
      "skip_global_deps":"yes",
      "sort":-1
    },
    "onnx": {
      "dataset_tags": [
        "image",
        "jpeg",
        "dataset"
      ],
      "ignore_return_code": "no",
      "run_time": {
        "fine_grain_timer_file": "tmp-ck-timer.json",
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ ../classify.py --image=$#dataset_path#$$#dataset_filename#$",
        "run_cmd_out1": "stdout.log",
        "run_cmd_out2": "stdout.err",
        "run_output_files": [
          "tmp-ck-timer.json"
        ]
      }
    },
    "tvm": {
      "dataset_tags": [
        "image",
        "jpeg",
        "dataset"
      ],
      "ignore_return_code": "no",
      "run_time": {
        "fine_grain_timer_file": "tmp-ck-timer.json",
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ ../classify.py --image=$#dataset_path#$$#dataset_filename#$",
        "run_output_files": [
          "tmp-ck-timer.json"
        ]
      },
      "run_deps": {
        "compiler-tvm": {
          "local": "yes",
          "name": "TVM compiler",
          "sort": 200,
          "tags": "compiler,tvm"
        },
        "lib-python-onnx": {
          "local": "yes",
          "name": "Python ONNX (to load model)",
          "sort": 125,
          "tags": "lib,python-package,onnx"
        }
      },
      "run_vars": {
        "USE_TVM": "yes"
      }
    },
    "api-onnx": {
      "ignore_return_code": "no",
      "run_time": {
        "fine_grain_timer_file": "tmp-ck-timer.json",
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ ../classify.py --image=$<<CK_IMAGE>>$",
        "run_output_files": [
          "tmp-ck-timer.json"
        ]
      }
    },
    "api-tvm": {
      "ignore_return_code": "no",
      "run_time": {
        "fine_grain_timer_file": "tmp-ck-timer.json",
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ ../classify.py --image=$<<CK_IMAGE>>$",
        "run_output_files": [
          "tmp-ck-timer.json"
        ]
      },
      "run_deps": {
        "compiler-tvm": {
          "local": "yes",
          "name": "TVM compiler",
          "sort": 200,
          "tags": "compiler,tvm"
        },
        "lib-python-onnx": {
          "local": "yes",
          "name": "Python ONNX (to load model)",
          "sort": 125,
          "tags": "lib,python-package,onnx"
        }
      },
      "run_vars": {
        "USE_TVM": "yes"
      }
    }
  },
  "run_deps": {
    "lib-python-matplotlib": {
      "local": "yes",
      "name": "Python Matplotlib library",
      "sort": 100,
      "tags": "lib,python-package,matplotlib"
    },
    "lib-python-scipy": {
      "local": "yes",
      "name": "Python SciPy library",
      "sort": 105,
      "tags": "lib,python-package,scipy"
    },
    "lib-python-onnx": {
      "local": "yes",
      "name": "Python ONNX (to load model)",
      "sort": 125,
      "tags": "lib,python-package,onnx"
    },
    "python": {
      "local": "yes",
      "name": "Python interpreter",
      "sort": 10,
      "tags": "compiler,python"
    },
    "lib-python-pillow": {
      "local": "yes",
      "name": "Python pillow library",
      "sort": 120,
      "tags": "lib,python-package,pillow"
    },
    "lib-python-numpy": {
      "local": "yes",
      "name": "Python NumPy library",
      "sort": 110,
      "tags": "lib,python-package,numpy"
    },
    "lib-python-onnxruntime": {
      "local": "yes",
      "name": "Python ONNX runtime (CPU)",
      "sort": 130,
      "tags": "lib,python-package,onnxruntime,cpu"
    },
    "lib-python-opencv": {
      "local": "yes",
      "name": "Python OpenCV library",
      "sort": 50,
      "tags": "lib,python-package,cv2"
    },
    "model": {
      "force_target_as_host": "yes",
      "local": "yes",
      "name": "Model (onnx, image classification)",
      "sort": 20,
      "tags": "ml-model,onnx,image-classification"
    }
  },
  "run_vars": {
     "TVM_OPT_LEVEL":3,
     "TVM_TARGET": "llvm -mcpu=znver2"
  },
  "tags": [
    "ml-task",
    "image-classification",
    "tvm"
  ]
}
