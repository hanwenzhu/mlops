{
  "no_compile": "yes",
  "no_target_file": "yes",
  "process_in_tmp": "yes",
  "program": "yes",
  "run_cmds": {
    "install-python-requirements": {
      "ignore_return_code": "no",
      "run_time": {
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ -m pip install -r ../requirements.txt",
        "min_run": "yes"
      },
      "sort":-1
    },
    "onnx_cpu": {
      "dataset_tags": [
        "image",
        "jpeg",
        "dataset"
      ],
      "ignore_return_code": "no",
      "run_time": {
        "fine_grain_timer_file": "tmp-ck-timer.json",
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ ../classify.py --image=$#dataset_path#$$#dataset_filename#$",
        "run_output_files": [
          "tmp-ck-timer.json"
        ]
      }
    },
    "tvm_cpu": {
      "dataset_tags": [
        "image",
        "jpeg",
        "dataset"
      ],
      "ignore_return_code": "no",
      "run_time": {
        "fine_grain_timer_file": "tmp-ck-timer.json",
        "run_cmd_main": "$<<CK_ENV_COMPILER_PYTHON_FILE>>$ ../classify.py --image=$#dataset_path#$$#dataset_filename#$",
        "run_output_files": [
          "tmp-ck-timer.json"
        ]
      },
      "run_deps": {
        "compiler-tvm": {
          "local": "yes",
          "name": "TVM compiler",
          "sort": 200,
          "tags": "compiler,tvm"
        },
        "lib-python-onnx": {
          "local": "yes",
          "name": "Python ONNX (to load model)",
          "sort": 125,
          "tags": "lib,python-package,onnx"
        }
      },
      "run_vars": {
        "USE_TVM": "yes"
      }
    }
  },
  "run_deps": {
    "lib-python-onnx": {
      "local": "yes",
      "name": "Python ONNX (to load model)",
      "sort": 125,
      "tags": "lib,python-package,onnx"
    },
    "python": {
      "local": "yes",
      "name": "Python interpreter",
      "sort": 10,
      "tags": "compiler,python"
    },
    "lib-python-pillow": {
      "local": "yes",
      "name": "Python pillow library",
      "sort": 120,
      "tags": "lib,python-package,pillow"
    },
    "lib-python-numpy": {
      "local": "yes",
      "name": "Python NumPy library",
      "sort": 110,
      "tags": "lib,python-package,numpy"
    },
    "lib-python-onnxruntime": {
      "local": "yes",
      "name": "Python ONNX runtime",
      "sort": 130,
      "tags": "lib,python-package,onnxruntime"
    },
    "lib-python-opencv": {
      "local": "yes",
      "name": "Python OpenCV library",
      "sort": 50,
      "tags": "lib,python-package,cv2"
    },
    "model": {
      "force_target_as_host": "yes",
      "local": "yes",
      "name": "Model (mlperf, onnx)",
      "sort": 20,
      "tags": "model,onnx,image-classification"
    }
  },
  "run_vars": {
    "CK_BATCH_SIZE": 1
  },
  "tags": [
    "ml-task",
    "image-classification",
    "tvm"
  ]
}
